<?xml version='1.0' encoding='utf-8'?>
<widget id="com.letiancenter.letianpsychological" version="2.1.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>乐天心理</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <access launch-external="yes" origin="tel:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-wechat" spec="^2.1.0">
        <variable name="WECHATAPPID" value="wx7407060a8eb038f8" />
    </plugin>
    <plugin name="cordova-plugin-camera" spec="^3.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.0" />
    <plugin name="jpush-phonegap-plugin" spec="git+https://github.com/jpush/jpush-phonegap-plugin.git">
        <variable name="APP_KEY" value="e8f17262e692f1f128637f40" />
    </plugin>
    <plugin name="cordova-plugin-alipay-v2" spec="^1.0.0">
        <variable name="APP_ID" value="2017031406210593" />
    </plugin>
    <plugin name="cordova-plugin-weibosdk" spec="^0.6.3">
        <variable name="WEIBO_APP_ID" value="4285108144" />
    </plugin>
    <engine name="android" spec="^6.3.0" />
</widget>
